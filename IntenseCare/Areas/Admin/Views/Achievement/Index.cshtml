@model IEnumerable < IntenseCare.Models.Datamodel>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

@section scripts{

    <link href="~/DataTables/DataTables-1.10.20/css/dataTables.bootstrap4.min.css" rel="stylesheet" />
    <script src="~/DataTables/jQuery-3.3.1/jquery-3.3.1.js"></script>
    <script src="~/DataTables/DataTables-1.10.20/js/dataTables.bootstrap4.min.js"></script>
    <script src="~/DataTables/DataTables-1.10.20/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" class="init">
        $(document).ready(function () {
            $("#example").DataTable();
        });
    </script>

}
    @*<p>
        @Html.ActionLink("Add Doctor Achievement", "Add", new { })
    </p>*@
<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-dark">List of Doctor Achievement Details</h6>
    </div>
    <div class="card-body">

        <div class="col-md-12">

            <table class=" table table-striped table-border" id="example">
                <thead class="thead-dark">
                    <tr>
                        <th>DoctorId</th>
                        <th>Achievement</th>
                        <th>AchievementDesCription</th>
                        <th>AchievementDate</th>

                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in Model)
                    {
                        <tr>
                            <td><a href="@Url.Action("Detail","Achievement",new { id = item.Achieve.AchievementId})">@item.Doctor.FirstName </a></td>
                            @*<td>@item.Doctor.FirstName</td>*@
                            <td>@item.Achieve.Achievement</td>
                            <td>@item.Achieve.AchievementDescription</td>
                            <td>@item.Achieve.AchievedDate</td>

                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>
        @* <script src="~/Scripts/jquery.unobtrusive-ajax.js"></script>*@





