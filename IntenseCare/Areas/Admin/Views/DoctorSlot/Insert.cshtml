<div class="container">
    <div class="card card-register mx-auto mt-5">
        <div class="card-header">Add Doctor Slot </div>
        <div class="card-body">
            <form method="post" name="frmCreate">
                <div class="form-group">
                    <div class="form-row">
                        <div class="col-lg-12">
                            <div class="form-label-group">
                                <input type="text" id="DoctorId" name="DoctorId" class="form-control" placeholder="Enter DoctorId" required="required" autofocus="autofocus">
                                <label for="DoctorId">DoctorId</label>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="form-row">
                        <div class="col-lg-12">
