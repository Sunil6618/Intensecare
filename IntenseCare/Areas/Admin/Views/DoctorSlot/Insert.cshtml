                                <input type="text" id="DayWeek" name="DayWeek" class="form-control" placeholder="Enter DayWeek" required="required">
                                <label for="DayWeek">DayWeek</label>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="form-row">
                        <div class="col-lg-12">
                            <div class="form-label-group">
                                <input type="text" id="StartTime" name="StartTime" class="form-control" placeholder="Enter StartTime" required="required">
                                <label for="StartTime">StartTime</label>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="form-group">
